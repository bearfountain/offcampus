<%= form_for(property) do |f| %>
  <%= f.error_messages %>
  <div class="field">

  </div>
  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </div>
  <div class="field">
    <%= f.label :laundry %><br />
    <%= f.check_box :laundry %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>